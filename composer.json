{
  "name": "epayco/epayco-php",
  "description": "Epayco PHP Library",
  "keywords": [
    "epayco",
    "payment processing",
    "api"
  ],
  "homepage": "https://epayco.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Epayco and contributors",
      "homepage": "https://github.com/epayco/epayco-php/contributors"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "php-coveralls/php-coveralls": "^2.1",
    "squizlabs/php_codesniffer": "^3.3",
    "symfony/process": "~3.4",
    "friendsofphp/php-cs-fixer": "2.17.1"
  },
  "autoload": {
    "psr-4": {
      "Epayco\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Epayco\\": [
        "tests/",
        "tests/Epayco/"
      ]
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
